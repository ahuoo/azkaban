      <div class="modal" id="messageDialog">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header" id="messageTitle">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h4 class="modal-title">Add a new configuration</h4>
            </div>
            <div class="modal-body" id="messageDiv">
              <fieldset>
                <div class="form-group">
                  <label for="lob" class="col-sm-3 control-label">Lob</label>
                  <div class="col-sm-9">
                    <input id="lob" type="text" name="source" val="${user}" class="form-control">
                  </div>
                  <br/> <br/> <br/>
                  <label for="source" class="col-sm-3 control-label">Source</label>
                  <div class="col-sm-9">
                    <input id="source" type="text" name="source" val="${user}" class="form-control">
                  </div>
                  <br/> <br/> <br/>
                 <label for="target" class="col-sm-3 control-label">Target</label>
                    <div class="col-sm-9">
                      <input id="target" type="text" name="target" val="${user}" class="form-control">
                 </div>
                  <br/> <br/> <br/>
                  <label for="target" class="col-sm-3 control-label">Execution Options</label>
                     <div class="col-sm-9">
                       <input id="execution_options" type="text" name="execution_options" val="${user}" class="form-control">
                  </div>
                </div>
              </fieldset>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
              <button id="submitCreateBtn" class="btn btn-primary">Create</button>
            </div>
          </div>
        </div>
      </div>
